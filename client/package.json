{
  "name": "vuejs-ts-todo",
  "version": "0.1.0",
  "private": true,
  "config": {
    "distPath": "./dist"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-conf-env vue-cli-service build --dest $npm_package_config_distPath",
    "clean": "cross-conf-env rimraf $npm_package_config_distPath",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "sbt:clean": "cross-conf-env rimraf $npm_package_config_distPath",
    "sbt:lint": "vue-cli-service lint --no-fix",
    "sbt:compile-only": "cross-conf-env vue-cli-service build --dest $npm_package_config_distPath --no-clean",
    "sbt:compile": "npm-run-all sbt:lint sbt:compile-only",
    "sbt:test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "element-ui": "^2.4.5",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.12",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.12",
    "@vue/cli-service": "^3.0.0-rc.12",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "cross-conf-env": "^1.1.2",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2",
    "vue-template-compiler": "^2.5.17"
  }
}
